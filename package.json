  "version": "1.0.0",
  "description": "DevStack - Your complete stack of essential developer utilities for JWT, crypto, text processing, and data conversion. Privacy-first, local-only processing.",
  "type": "module",
  "homepage": "https://vamsidevbits.github.io/devstack",
  "repository": {
    "type": "git",
    "url": "https://github.com/vamsidevbits/devstack.git"
  },
  "author": "Vamsi Krishna",
  "license": "MIT",
  "keywords": [
    "devstack",
    "jwt",
    "developer-tools",
    "crypto",
    "base64",
    "json",
    "hash",
    "utilities",
    "privacy-first",
    "react",
    "vite",
    "tailwindcss"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
